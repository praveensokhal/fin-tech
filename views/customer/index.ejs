<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../partials/admin_header'); %>
    </header>

    <main>
   
            <% if (messages.error) { %>
                <p style="color:red"><%- messages.error %></p>
                <% } %>
            <% if (messages.success) { %>
                <p class="alert alert-success mt-4">
                    <%- messages.success %>
                </p>
                <% } %>
            <br>
           <%- include(`../${main.page}`,{data:main.data})%>
    </main>
    
    <footer>
        <%- include('../partials/footer'); %>
    </footer>
    
    </body>
    
    </html>